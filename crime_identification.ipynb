    "from sklearn.model_selection import ShuffleSplit, cross_val_score\n",
    "import numpy as np\n",
    "\n",
    "# Define the Random Forest model with optimized parameters for cross-validation\n",
    "# Using n_jobs=-1 to parallelize the process and max_samples for memory efficiency\n",
    "rf_cv_model = RandomForestClassifier(\n",
    "    n_estimators=100,\n",
    "    max_samples=0.3,  # Use a subset of data for each tree to save memory/time\n",
    "    random_state=42,\n",
    "    n_jobs=-1  # Use all available cores\n",
    ")\n",
    "\n",
    "# Configure ShuffleSplit for cross-validation\n",
    "# It's efficient as it creates a specified number of independent train/test splits\n",
    "cv_strategy = ShuffleSplit(n_splits=5, test_size=0.2, random_state=42)\n",
    "\n",
    "# Perform cross-validation\n",
    "# n_jobs=-1 here will parallelize the cross-validation folds\n",
    "cv_scores = cross_val_score(\n",
    "    rf_cv_model,\n",
    "    X,\n",
    "    y,\n",
    "    cv=cv_strategy,\n",
    "    scoring='accuracy',\n",
    "    n_jobs=-1  # Use all available cores for running folds in parallel\n",
    ")\n",
    "\n",
    "# Print the results in a more readable format\n",
    "print(\"Cross-Validation Accuracy Scores for each fold:\")\n",
    "print(cv_scores)\n",
    "print(\"\\n--- Summary ---\")\n",
    "print(f\"Mean CV Accuracy: {np.mean(cv_scores):.4f}\")\n",
    "print(f\"Standard Deviation of CV Accuracy: {np.std(cv_scores):.4f}\")"
   ]

    "# Turn into sorted dataframe\n",
    "target_corr_report = (\n",
    "    target_corr\n",
    "        .abs()\n",
    "        .sort_values(ascending=False)\n",
    "        .rename(\"Correlation_with_Crime_Class\")\n",
    "        .to_frame()\n",
    ")\n",
    "\n",
    "target_corr_report.head(20)\n"
